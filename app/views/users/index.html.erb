<h1>Users</h1>
<div class="sign_out_button">
   <button>
     <%= link_to 'Sign out',  destroy_user_session_path, method: :delete %>
   </button>
 </div>
<div class="container">
<div class="user_profile">
<div class="photos">
  <% @users.each do |user| %>
    <ul class="photo"> 
   <li> <img src="<%= user.photo %>"> </li>
   </ul>
    <% end %>
</div>

<div class="names_posts">
  <% @users.each do |user| %>
    <ul class="name_post"> 
   <li class="list_name"> <%= link_to user.name, user_path(user) %> </li>
   <li class="list_countpost">Number of posts: <%= user.postscounter %> </li>
   </ul>
    <% end %>
</div>
</div>
</div>